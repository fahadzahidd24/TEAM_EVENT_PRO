{
  "expo": {
    "name": "TeamEvent Pro",
    "slug": "teamevent-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2e323b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#2e323b"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ],
      "package": "com.fahadzahidd24.TEAM_EVENT_PRO"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidNavigationBar": {
      "visible": "sticky"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to your gallery."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "This app needs access to your camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6f410670-8628-4bcf-b0bf-99cdb3d30a53"
      }
    },
    "owner": "fahadzahidd24"
  }
}
